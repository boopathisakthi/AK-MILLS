            <div class="kt-grid__item kt-grid__item--fluid kt-wizard-v1__wrapper">
                <!--begin: Form Wizard Form-->
                <form class="kt-form" id="kt_contacts_add_form2"  novalidate="novalidate" style="margin-top:-80px">
                    <!--begin: Form Wizard Step 1-->
                    <div class="kt-wizard-v1__content" data-ktwizard-type="step-content">
                        <!-- <div class="kt-heading kt-heading--md">Branch Details:</div> -->
                        <div class="kt-section kt-section--first">
                            <div class="kt-wizard-v1__form">
                                <div class="row">
                                    <div class="col-xl-12">
                                        <div class="kt-section__body">

                                            <div class="form-group row">
                                                <div class="col-lg-4">
                                                    <label>Company Name:</label>
                                                    <input class="form-control" name="companyname" placeholder="Enter Company Name"
                                                        type="text" id="txtcompanyname">
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Gstno:</label>
                                                    <input class="form-control" name="gstno" placeholder="Enter Gstno" type="text"
                                                        id="txtgstno">
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Panno:</label>
                                                    <input class="form-control" name="panno" placeholder="Enter Panno" type="text"
                                                        id="txtpanno">
                                                </div>

                                            </div>
                                            <div class="form-group row">

                                                <div class="col-lg-4">
                                                    <label>Address:</label>
                                                    <input class="form-control" name="address" placeholder="Enter Address" type="text"
                                                        id="txtaddress">
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Pincode:</label>
                                                    <input class="form-control" name="pincode" maxlength="6" placeholder="Enter Pincode" type="text"
                                                        id="txtpincode">
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>City:</label>
                                                    <select id="ddlcity" name="city" class="form-control"></select>

                                                </div>
                                            </div>
                                            <div class="form-group row">

                                                <div class="col-lg-4">
                                                    <label>State:</label>
                                                    <select id="ddlstate" name="state" class="form-control"></select>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Telephone No:</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend"><span
                                                                class="input-group-text"><i
                                                                    class="la la-phone"></i></span></div>
                                                        <input type="text" name="telephoneno" id="txttelephoneno" class="form-control"
                                                            placeholder="Phone" aria-describedby="basic-addon1">
                                                    </div>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Mobile No:</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend"><span
                                                                class="input-group-text"><i
                                                                    class="la la-phone"></i></span></div>
                                                        <input type="text" name="mobile" id="txtmobileno" maxlength="10" class="form-control"
                                                            placeholder="Mobile No" aria-describedby="basic-addon1">
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group row">

                                                <div class="col-lg-4">
                                                    <label>Email Address:</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend"><span
                                                                class="input-group-text"><i class="la la-at"></i></span>
                                                        </div>
                                                        <input type="text" name="email" class="form-control" id="txtemail"
                                                            placeholder="enter email" aria-describedby="basic-addon1">
                                                    </div>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Bill Code:</label>
                                                    <input type="text" name="billcode" class="form-control" id="txtbillcode"
                                                        placeholder="Bill code" aria-describedby="basic-addon1">
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Credit Days:</label>
                                                    <input type="text" name="creditdays" class="form-control" id="txtcreditdays"
                                                        placeholder="Credit Days" aria-describedby="basic-addon1">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--end: Form Wizard Step 1-->

                    <!--begin: Form Wizard Step 2-->
                    <div class="kt-wizard-v1__content" data-ktwizard-type="step-content" data-ktwizard-state="current">
                        <div class="kt-section kt-section--first">
                            <div class="kt-wizard-v1__form">
                                <div class="row">
                                    <div class="col-xl-12">
                                        <div class="kt-section__body">

                                            <div class="form-group row">
                                                <label class="col-xl-3 col-lg-3 col-form-label">Bank Name</label>
                                                <div class="col-lg-9 col-xl-9">
                                                    <input class="form-control" name="bankname" placeholder="Enter Bank Name"
                                                        id="txtbankname" type="text">
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <label class="col-xl-3 col-lg-3 col-form-label">Branch Name</label>
                                                <div class="col-lg-9 col-xl-9">
                                                    <input class="form-control" name="branchname" placeholder="Enter Branch Name"
                                                        id="txtbranchname" type="text">
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <label class="col-xl-3 col-lg-3 col-form-label">Account Holder
                                                    Name</label>
                                                <div class="col-lg-9 col-xl-9">
                                                    <input class="form-control" name="accountholdername" placeholder="Enter Account Holder Name"
                                                        id="txtaccountname" type="text">
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <label class="col-xl-3 col-lg-3 col-form-label">Account Number</label>
                                                <div class="col-lg-9 col-xl-9">
                                                    <input class="form-control" name="accountnumber" placeholder="Enter Account Number"
                                                        type="text" id="txtaccountnumber">
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <label class="col-xl-3 col-lg-3 col-form-label">IFSC Code</label>
                                                <div class="col-lg-9 col-xl-9">
                                                    <input class="form-control" name="ifsc_code" placeholder="Enter IFSC Code"
                                                        id="txtifscno" type="text">
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--end: Form Wizard Step 2-->

                    <!--begin: Form Wizard Step 3-->
                    <div class="kt-wizard-v1__content" data-ktwizard-type="step-content">

                        <div class="kt-form__section kt-form__section--first">
                            <div class="kt-wizard-v1__form">
                                <div class="form-group row">
                                    <div class="col-lg-4">
                                        <label>Website logo</label>
                                        <div class="col-lg-9 col-xl-6">
                                            <img src="/appfiles/CompanyImages/default.png" class="" style="margin:10px;" height="150" width="150" id="imagePreview" />
                                            <input type="file" id="imguploader" name="kt_contacts_add_avatar" onchange="ShowImagePreview(this, document.getElementById('imagePreview'))">
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <label>Mobile logo</label>
                                        <div class="col-lg-9 col-xl-6">
                                            <img src="/appfiles/CompanyImages/default.png" class="" style="margin:10px;" height="150" width="150" id="ipmobileheader" />
                                            <input type="file" id="iumobileheader" name="kt_contacts_add_avatar1" accept="image/jpeg, image/png" onchange="ShowImagePreview(this, document.getElementById('ipmobileheader'))" />
                                        </div>

                                    </div>
                                    <div class="col-lg-4">
                                        <label>Mobile logo</label>
                                        <div class="col-lg-9 col-xl-6">
                                            <img src="/appfiles/CompanyImages/default.png" class="" style="margin:10px;" height="150" width="150" id="ipreportlogo" />
                                            <input type="file" id="ulreportlogo" name="kt_contacts_add_avatar1" accept="image/jpeg, image/png" onchange="ShowImagePreview(this, document.getElementById('ipreportlogo'))" />
                                        </div>
                                   </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!--end: Form Wizard Step 3-->


                    <!--begin: Form Actions -->
                    <div class="kt-form__actions" >
                        <div class="btn btn-secondary btn-md btn-tall btn-wide kt-font-bold kt-font-transform-u"
                            data-ktwizard-type="action-prev">
                            Previous
                        </div>
                        <input type="submit" 
                            class="btn btn-success btn-md btn-tall btn-wide kt-font-bold kt-font-transform-u"
                            data-ktwizard-type="action-submit">
                        <!-- <div  class="btn btn-success btn-md btn-tall btn-wide kt-font-bold kt-font-transform-u" data-ktwizard-type="action-submit">
                            Submit
                        </div> -->
                        <div class="btn btn-brand btn-md btn-tall btn-wide kt-font-bold kt-font-transform-u"
                            data-ktwizard-type="action-next">
                            Next Step
                        </div>
                    </div>
                    <!--end: Form Actions -->
                </form>
                <!--end: Form Wizard Form-->
                <input type="hidden" id="hfsysid">
            </div>
        </div>
    </div>

</div>

