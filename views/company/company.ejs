<div class="kt-portlet list">
    <div class="kt-portlet__head">
        <div class="kt-portlet__head-label">
            <h3 class="kt-portlet__head-title">
                Company List
            </h3>
        </div>
        <div class="kt-portlet__head-toolbar">
            <button type="button" onclick="show()" id="btnshow" class="btn btn-label-brand btn-sm"><i
                    class="fa fa-plus"></i>&nbsp;Add</button>
        </div>
    </div>
    <div class="kt-portlet__body">
        <table class="table table-sm table-striped- table-bordered table-hover table-checkable" id="gvlist">
            <thead>
                <tr>
                    <th>Sno</th>
                    <th>companyname</th>
                    <th>gstno</th>
                    <th>panno</th>
                    <th>Mobile</th>
                    <th>Actions</th>
                </tr>
            </thead>
        </table>
    </div>
</div>
